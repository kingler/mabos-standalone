<?xml version="1.0"?>
<rdf:RDF 
    xmlns="http://example.org/ontology#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <owl:Ontology rdf:about="http://example.org/ontology"/>

    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/ontology#Business"/>
    <owl:Class rdf:about="http://example.org/ontology#Product"/>
    <owl:Class rdf:about="http://example.org/ontology#Service"/>
    <owl:Class rdf:about="http://example.org/ontology#Customer"/>
    <owl:Class rdf:about="http://example.org/ontology#Employee"/>
    <owl:Class rdf:about="http://example.org/ontology#Department"/>
    <owl:Class rdf:about="http://example.org/ontology#Project"/>
    <owl:Class rdf:about="http://example.org/ontology#Goal"/>
    <owl:Class rdf:about="http://example.org/ontology#Task"/>
    <owl:Class rdf:about="http://example.org/ontology#Resource"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasProduct">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Product"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasService">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Service"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasCustomer">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Customer"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasEmployee">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Employee"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasDepartment">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Department"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasProject">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Project"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasGoal">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Goal"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#hasTask">
        <rdfs:domain rdf:resource="http://example.org/ontology#Project"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Task"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/ontology#usesResource">
        <rdfs:domain rdf:resource="http://example.org/ontology#Task"/>
        <rdfs:range rdf:resource="http://example.org/ontology#Resource"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://example.org/ontology#name">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/ontology#description">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/ontology#industry">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/ontology#size">
        <rdfs:domain rdf:resource="http://example.org/ontology#Business"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

</rdf:RDF>
