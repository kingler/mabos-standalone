<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns="http://example.org/business-ontology#"
     xml:base="http://example.org/business-ontology/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:sbvr="http://example.org/sbvr#">
    <owl:Ontology rdf:about="http://example.org/business-ontology"/>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <owl:AnnotationProperty rdf:about="http://example.org/sbvr#designation"/>
    <owl:AnnotationProperty rdf:about="http://example.org/sbvr#necessity"/>
    <owl:AnnotationProperty rdf:about="http://example.org/sbvr#possibility"/>
    <owl:AnnotationProperty rdf:about="http://example.org/sbvr#obligation"/>
    <owl:AnnotationProperty rdf:about="http://example.org/sbvr#permission"/>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
   
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasCompany">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Employment"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Company"/>
        <rdfs:comment>Relates an employment to its company</rdfs:comment>
        <rdfs:label>has company</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasDefinition">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Definition"/>
        <rdfs:comment>The definition represents the concept</rdfs:comment>
        <rdfs:label>has definition</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasEmployee">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Employment"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Employee"/>
        <rdfs:comment>Relates an employment to its employee</rdfs:comment>
        <rdfs:label>has employee</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasEmployeeID">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Employee"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#EmployeeID"/>
        <rdfs:comment>Relates an employee to their unique identifier</rdfs:comment>
        <rdfs:label>has employee ID</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasVerbConceptRole">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#VerbConcept"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#VerbConceptRole"/>
        <rdfs:comment>The role is an abstraction of a thing playing a part in an instance of the verb concept</rdfs:comment>
        <rdfs:label>has verb concept role</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#managerRole">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#EmployeeManagesEmployee"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Employee"/>
        <rdfs:comment>The role of the employee who is the manager in the management relationship</rdfs:comment>
        <rdfs:label>manager role</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#represents">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Expression"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Meaning"/>
        <rdfs:comment>The expression portrays or signifies the meaning</rdfs:comment>
        <rdfs:label>represents</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#subordinateRole">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#EmployeeManagesEmployee"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Employee"/>
        <rdfs:comment>The role of the employee who is managed in the management relationship</rdfs:comment>
        <rdfs:label>subordinate role</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#worksFor">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Employee"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Company"/>
        <rdfs:comment>Represents the employment relationship between an employee and a company</rdfs:comment>
        <rdfs:label>works for</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasFact">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Proposition"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Fact"/>
        <rdfs:comment>Relates a proposition to a fact</rdfs:comment>
        <rdfs:label>has fact</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasBusinessPolicy">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Company"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#BusinessPolicy"/>
        <rdfs:comment>Relates a company to its business policies</rdfs:comment>
        <rdfs:label>has business policy</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasCategorizationScheme">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#CategorizationScheme"/>
        <rdfs:comment>Relates a concept to its categorization scheme</rdfs:comment>
        <rdfs:label>has categorization scheme</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasSegmentation">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Market"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Segmentation"/>
        <rdfs:comment>Relates a market to its segmentation</rdfs:comment>
        <rdfs:label>has segmentation</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasSemanticCommunity">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#SemanticCommunity"/>
        <rdfs:comment>Relates a concept to its semantic community</rdfs:comment>
        <rdfs:label>has semantic community</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasName">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the company</rdfs:comment>
        <rdfs:label>has name</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasDesignation">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Designation"/>
        <rdfs:comment>Relates a concept to its designation</rdfs:comment>
        <rdfs:label>has designation</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasAuthority">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#ElementOfGuidance"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Authority"/>
        <rdfs:comment>Relates an element of guidance to its authority</rdfs:comment>
        <rdfs:label>has authority</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasCategory">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Category"/>
        <rdfs:comment>Relates a concept to its category</rdfs:comment>
        <rdfs:label>has category</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#hasQuantification">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Quantification"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Quantification"/>
        <rdfs:comment>Relates a concept to its quantification</rdfs:comment>
        <rdfs:label>has quantification</rdfs:label>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#whetherOrNotFormulationHasConsequent">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#WhetherOrNotFormulation"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Consequent"/>
        <rdfs:comment>Relates a whether-or-not formulation to its consequent</rdfs:comment>
        <rdfs:label>whether-or-not formulation has consequent</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/business-ontology#whetherOrNotFormulationHasInconsequent">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#WhetherOrNotFormulation"/>
        <rdfs:range rdf:resource="http://example.org/business-ontology#Inconsequent"/>
        <rdfs:comment>Relates a whether-or-not formulation to its inconsequent</rdfs:comment>
        <rdfs:label>whether-or-not formulation has inconsequent</rdfs:label>
    </owl:ObjectProperty>
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <owl:DatatypeProperty rdf:about="http://example.org/business-ontology#age">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The age of an employee</rdfs:comment>
        <rdfs:label>age</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/business-ontology#age">
        <rdfs:domain rdf:resource="http://example.org/business-ontology#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Represents the age of a person</rdfs:comment>
        <rdfs:label>age</rdfs:label>
    </owl:DatatypeProperty>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <owl:Class rdf:about="http://example.org/business-ontology#AtLeastNQuantification">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Quantification"/>
        <rdfs:comment>Quantification that has a minimum cardinality</rdfs:comment>
        <rdfs:label>At-Least-N Quantification</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#AtMostNQuantification">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Quantification"/>
        <rdfs:comment>Quantification that has a maximum cardinality</rdfs:comment>
        <rdfs:label>At-Most-N Quantification</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#AtomicFormulation">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#LogicalFormulation"/>
        <rdfs:comment>Logical formulation that is based on a verb concept and that has a role binding of each role of the verb concept</rdfs:comment>
        <rdfs:label>Atomic Formulation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#CategorizationScheme">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:comment>Scheme for partitioning things in the extension of a given general concept into the extensions of categories of that general concept</rdfs:comment>
        <rdfs:label>Categorization Scheme</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Company">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <sbvr:designation>company</sbvr:designation>
        <rdfs:comment>Organization that operates commercially in a specific business</rdfs:comment>
        <rdfs:label>Company</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Concept">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Meaning"/>
        <rdfs:comment>Unit of knowledge created by a unique combination of characteristics</rdfs:comment>
        <rdfs:label>Concept</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Definition">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Representation"/>
        <rdfs:comment>Representation of a concept by a descriptive statement which serves to differentiate it from related concepts</rdfs:comment>
        <rdfs:label>Definition</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Employee">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://example.org/business-ontology#age"/>
                <owl:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</owl:minInclusive>
            </owl:Restriction>
        </rdfs:subClassOf>
        <sbvr:designation>employee</sbvr:designation>
        <sbvr:necessity>Each employee has exactly one employee ID</sbvr:necessity>
        <sbvr:necessity>Each employee works for exactly one company</sbvr:necessity>
        <sbvr:necessity>It is necessary that each employee is at least 18 years old</sbvr:necessity>
        <rdfs:comment>Person who works for a company in return for wages or a salary</rdfs:comment>
        <rdfs:label>Employee</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#EmployeeID">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <sbvr:designation>employee ID</sbvr:designation>
        <rdfs:comment>Unique identifier assigned to an employee</rdfs:comment>
        <rdfs:label>Employee ID</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#EmployeeManagesEmployee">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#VerbConcept"/>
        <sbvr:designation>employee manages employee</sbvr:designation>
        <rdfs:comment>Verb concept representing the management relationship between employees</rdfs:comment>
        <rdfs:label>Employee Manages Employee</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Employment">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <sbvr:designation>employment</sbvr:designation>
        <rdfs:comment>Objectification of the verb concept &apos;employee works for company&apos;</rdfs:comment>
        <rdfs:label>Employment</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Expression">
        <rdfs:comment>Something that expresses or communicates, but considered independently of its interpretation</rdfs:comment>
        <rdfs:label>Expression</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#ExistentialQuantification">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Quantification"/>
        <rdfs:comment>Quantification that has the minimum cardinality 1</rdfs:comment>
        <rdfs:label>Existential Quantification</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Fact">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Proposition"/>
        <rdfs:comment>Proposition that is taken as true</rdfs:comment>
        <rdfs:label>Fact</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#LogicalFormulation">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#SemanticFormulation"/>
        <rdfs:comment>Semantic formulation that formulates a proposition</rdfs:comment>
        <rdfs:label>Logical Formulation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#LogicalOperation">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#LogicalFormulation"/>
        <rdfs:comment>Logical formulation that formulates a meaning based on only the truth or falseness of the meanings of one or more other logical formulations</rdfs:comment>
        <rdfs:label>Logical Operation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Meaning">
        <rdfs:comment>What is meant by a word, sign, statement, or description; what someone intends to express or what someone understands</rdfs:comment>
        <rdfs:label>Meaning</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#MinimumAge">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <sbvr:designation>minimum age</sbvr:designation>
        <rdfs:comment>The minimum age required for employment</rdfs:comment>
        <rdfs:label>Minimum Age</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Quantification">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:comment>Concept that represents a restriction on the number of things that can be in the extension of a concept</rdfs:comment>
        <rdfs:label>Quantification</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Proposition">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Meaning"/>
        <rdfs:comment>Meaning that is either true or false</rdfs:comment>
        <rdfs:label>Proposition</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Representation">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Meaning"/>
        <rdfs:comment>Meaning that is a sign or symbol for another meaning</rdfs:comment>
        <rdfs:label>Representation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#SemanticFormulation">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Meaning"/>
        <rdfs:comment>Meaning that formulates another meaning</rdfs:comment>
        <rdfs:label>Logical Formulation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#VerbConcept">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:comment>Concept that represents a relation between things</rdfs:comment>
        <rdfs:label>Verb Concept</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#VerbConceptRole">
        <rdfs:subClassOf rdf:resource="http://example.org/business-ontology#Concept"/>
        <rdfs:comment>Abstraction of a thing playing a part in an instance of a verb concept</rdfs:comment>
        <rdfs:label>Verb Concept Role</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#BusinessPolicy">
        <rdfs:comment>Policy that governs business operations</rdfs:comment>
        <rdfs:label>Business Policy</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Segmentation">
        <rdfs:comment>Division of a market into distinct groups</rdfs:comment>
        <rdfs:label>Segmentation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#SemanticCommunity">
        <rdfs:comment>A community that shares a common understanding</rdfs:comment>
        <rdfs:label>Semantic Community</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#VocabularyNamespace">
        <rdfs:comment>Namespace for vocabulary definitions</rdfs:comment>
        <rdfs:label>Vocabulary Namespace</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#SpeechCommunity">
        <rdfs:comment>A community that shares a common language</rdfs:comment>
        <rdfs:label>Speech Community</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#LogicalOperation">
        <rdfs:comment>Operation that involves logical reasoning</rdfs:comment>
        <rdfs:label>Logical Operation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Concept1SpecializesConcept2">
        <rdfs:comment>Relationship indicating specialization between concepts</rdfs:comment>
        <rdfs:label>Concept Specialization</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#InstantiationFormulation">
        <rdfs:comment>Formulation that binds to a target</rdfs:comment>
        <rdfs:label>Instantiation Formulation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Quantification">
        <rdfs:comment>Concept that represents a restriction on the number of things</rdfs:comment>
        <rdfs:label>Quantification</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#ElementOfGuidance">
        <rdfs:comment>Guidance element in the SBVR context</rdfs:comment>
        <rdfs:label>Element of Guidance</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Authority">
        <rdfs:comment>Entity that has the power to enforce rules</rdfs:comment>
        <rdfs:label>Authority</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Designation">
        <rdfs:comment>Signifier for a concept</rdfs:comment>
        <rdfs:label>Designation</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/business-ontology#Category">
        <rdfs:comment>Category for organizing concepts</rdfs:comment>
        <rdfs:label>Category</rdfs:label>
    </owl:Class>
    
    <!-- Add missing classes -->
    <owl:Class rdf:about="http://example.org/business-ontology#VocabularyNamespaceIsForLanguage">
        <rdfs:comment>Namespace specific to a language</rdfs:comment>
        <rdfs:label>Vocabulary Namespace Is For Language</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#VocabularyNamespaceIsSpecificToSubjectField">
        <rdfs:comment>Namespace specific to a subject field</rdfs:comment>
        <rdfs:label>Vocabulary Namespace Is Specific To Subject Field</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#WhetherOrNotFormulation">
        <rdfs:comment>Formulation that expresses a condition</rdfs:comment>
        <rdfs:label>Whether Or Not Formulation</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#AdoptedDefinition">
        <rdfs:comment>Definition that has been adopted</rdfs:comment>
        <rdfs:label>Adopted Definition</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#Advice">
        <rdfs:comment>Advice given in a specific context</rdfs:comment>
        <rdfs:label>Advice</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#ElementOfGuidancePermitsStateOfAffairs">
        <rdfs:comment>Guidance element that permits a state of affairs</rdfs:comment>
        <rdfs:label>Element Of Guidance Permits State Of Affairs</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#ModalFormulationEmbedsLogicalFormulation">
        <rdfs:comment>Modal formulation that embeds a logical formulation</rdfs:comment>
        <rdfs:label>Modal Formulation Embeds Logical Formulation</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/business-ontology#Variable">
        <rdfs:comment>Variable in a logical formulation</rdfs:comment>
